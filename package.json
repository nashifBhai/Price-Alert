  "description": "Description for invioso",
  "private": true,
  "scripts": {
    "ci-build": "node --max-old-space-size=8192 node_modules/webpack/bin/webpack.js --env env=stage",
    "clean": "del-cli src/main/webapp/dist/ src/main/webapp/scripts/bundles src/main/webapp/scripts/ui/*.module.scss.d.ts",
    "build": "webpack",
    "build:analyze": "npm run build -- --env analyze",
    "dev": "webpack --env env=dev",
    "dev:analyze": "npm run dev -- --env analyze",
    "lint": "npm run lint:legacy && npm run lint:react",
    "lint:quiet": "npm run lint:legacy -- --quiet && npm run lint:react -- --quiet",
    "lint:legacy": "eslint --no-eslintrc -c .eslintrc-angular.json src/main/webapp/scripts/",
    "lint:react": "eslint src/main/webapp/scripts/ui/ --ext .tsx,.ts",
    "prebuild": "npm run clean && npm run lint:quiet && npm run format && npm run test",
    "predev": "npm run clean",
    "prepare": "husky install",
    "start": "webpack serve --env env=dev",
    "format": "npx prettier --check src/main/webapp/scripts/ui/** --loglevel warn",
    "format:fix": "npx prettier --write src/main/webapp/scripts/ui/**",
    "test": "npm run test:react",
    "test:legacy": "karma start ./src/test/javascript/karma.conf.js",
    "test:react": "jest --coverage",
    "automationSanitySuite": "node node_modules/protractor/bin/webdriver-manager update && protractor src/test/qa_automation/conf.js --specs src/test/qa_automation/specs/sanitySuite/sanity.spec.js",
    "test:uiAutomation": "node_modules/cypress/bin/cypress run -- --spec ./cypress/integration/ui_automation_spec.js --headless --browser chrome",
    "cy:verify": "node_modules/cypress/bin/cypress verify",
    "test:uiAutomationAWS": "/usr/lib/node_modules/cypress/bin/cypress run -- --spec ./cypress/integration/ui_automation_spec.js --headless --browser chrome",
    "cy:verifyAWS": "/usr/lib/node_modules/cypress/bin/cypress verify",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -s public"
  },
